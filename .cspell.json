{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    ".vscode",
    "megalinter",
    "package-lock.json",
    "requirements.txt",
    "roles/**",
    ".github/**",
    "Dockerfile(-.+)?",
    "report"
  ],
  "language": "en",
  "noConfigSearch": true,
  "words": [
    "Andrieu",
    "Alban",
    "albandri",
    "albandrieu",
    "virtualenv",
    "tocstop",
    "toc",
    "jusmundi",
    "Mundi",
    "ORGANISATION",
    "pgclient",
    "hadolint",
    "readmemd",
    "alertapi",
    "postgresql",
    "secretlint",
    "scaleway"
  ],
  "languageSettings": [
  {
    "languageId": "markdown",
    "caseSensitive": true
  },
  {
    "languageId": "javascript",
    "caseSensitive": false
  }
  ],
  "version": "0.2"
}
